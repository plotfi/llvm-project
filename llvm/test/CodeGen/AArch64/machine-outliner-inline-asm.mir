# RUN: llc -run-pass=machine-outliner -enable-machine-outliner -verify-machineinstrs %s -o - | FileCheck %s
--- |
  target datalayout = "e-m:o-i64:64-i128:128-n32:64-S128"
  target triple = "arm64-apple-ios9.0.0"

  define void @foo(i32, i32, i32) #0  {
    ret void
  }
  define void @hoo(i32, i32, i32) #0  {
    ret void
  }
  define void @bar(i32, i32, i32) #0  {
    ret void
  }

  attributes #0 = { minsize optsize ssp uwtable noredzone }
...
---

name:            foo
tracksRegLiveness: true
body:             |
  bb.0:
    liveins: $x0, $x1, $x2, $lr
    INLINEASM &"mov x1, x2", 0
    INLINEASM &"mov x0, x1", 0
    RET undef $lr
...
---

name:            hoo
tracksRegLiveness: true
body:             |
  bb.0:
    liveins: $x0, $x1, $x2, $lr
    INLINEASM &"mov x1, x2", 0
    INLINEASM &"mov x0, x1", 0
    RET undef $lr
...
---

name:            bar
tracksRegLiveness: true
body:             |
  bb.0:
    liveins: $x0, $x1, $x2, $lr
    INLINEASM &"mov x1, x2", 0
    INLINEASM &"mov x0, x1", 0
    RET undef $lr
...

# CHECK: name: OUTLINED_FUNCTION_
